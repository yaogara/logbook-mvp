{
  "version": 3,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "continue": true
    },
    {
      "src": "/sw.js",
      "dest": "/sw.js",
      "continue": true
    },
    {
      "src": "/manifest.webmanifest",
      "dest": "/manifest.webmanifest",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "VITE_SUPABASE_URL": "@vite_supabase_url",
      "VITE_SUPABASE_ANON_KEY": "@vite_supabase_anon_key"
    }
  }
}